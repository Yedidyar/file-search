{
  "name": "docs",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/docs",
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vitepress dev",
        "cwd": "apps/docs"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vitepress build",
        "cwd": "apps/docs"
      },
      "outputs": ["{projectRoot}/.vitepress/dist"]
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vitepress preview",
        "cwd": "apps/docs"
      },
      "dependsOn": ["build"]
    }
  },
  "tags": ["type:app", "scope:docs"]
}
